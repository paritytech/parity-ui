    "rucksack-css": "^0.8.6",
    "shelljs": "^0.7.0",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-copy-on-done-plugin": "0.0.9",
    "webpack-error-notification": "^0.1.6",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-httpolyglot-server": "^0.2.0"
  },
  "dependencies": {
    "chrome-badge-animator": "0.0.0",
