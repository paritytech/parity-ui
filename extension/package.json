{
  "name": "parity-web-extension",
  "version": "1.1.1",
  "description": "Parity Web Extension",
  "scripts": {
    "link": "rm -rf ../signer/web/node_modules && npm link ../signer/web && rm -rf ../components/node_modules && npm link ../components",
    "start": "npm run devBuild -- -w",
    "devBuild": "webpack --progress --colors",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --progress --colors --bail",
    "compress": "node scripts/compress",
    "genZip": "npm run build && npm run compress",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "lint": "eslint . --ignore-path .gitignore",
    "test": "mocha \"chrome/**/*.spec.js\""
  },
  "author": "Ethcore <admin@ethcore.io>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethcore/parity-ui.git"
  },
  "bugs": {
    "url": "https://github.com/ethcore/parity-ui/issues"
  },
  "homepage": "https://github.com/ethcore/parity-ui#readme",
  "keywords": [
    "parity-sysui"
  ],
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-lodash": "^3.2.2",
    "babel-plugin-transform-decorators-legacy": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "bignumber.js": "^2.3.0",
    "chai": "^3.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "crx": "^3.0.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.3",
    "eslint-config-semistandard": "^6.0.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.5.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.9.0",
    "json-loader": "0.5.4",
    "minimist": "^1.2.0",
